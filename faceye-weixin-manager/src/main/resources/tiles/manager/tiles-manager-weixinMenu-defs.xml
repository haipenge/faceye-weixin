<tiles-definitions>
    <!-- 模块:weixin 业务对像:WeixinMenu 管理后台 tiles模板配置 -->
    <!-- 作者:haipege -->
    <!-- WeixinMenu 管理 首页(列表页) -->
	<definition name="weixin.weixinMenu.manager" extends="default.manager">
	    <put-attribute name="default-center" value="weixin.weixinMenu.center"></put-attribute>
	</definition>
	<definition name="weixin.weixinMenu.center" extends="default.center">
	    <put-attribute name="default-center-right" value="/component/weixin/weixinMenu/weixinMenu_center.jsp"></put-attribute>
	</definition>
	
	<!-- WeixinMenu 新增,编辑页面 -->
	<definition name="weixin.weixinMenu.update" extends="default.manager">
