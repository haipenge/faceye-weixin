	<definition name="weixin.weixinMenu.update.center" extends="default.center">
	    <put-attribute name="default-center-right" value="/component/weixin/weixinMenu/weixinMenu_update.jsp"></put-attribute>
	</definition>
	
	<!-- WeixinMenu 明细页 -->
	<definition name="weixin.weixinMenu.detail" extends="default.manager">
	    <put-attribute name="default-center" value="weixin.weixinMenu.detail.center"></put-attribute>
	</definition>
	<definition name="weixin.weixinMenu.detail.center" extends="default.center">
	    <put-attribute name="default-center-right" value="/component/weixin/weixinMenu/weixinMenu_detail.jsp"></put-attribute>
